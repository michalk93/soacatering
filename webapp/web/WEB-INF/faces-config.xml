<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/public/menu.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editCourse</from-outcome>
            <to-view-id>/manager/course/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/public/categories.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editCategory</from-outcome>
            <to-view-id>/manager/category/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/user/subscription/list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editSubscription</from-outcome>
            <to-view-id>/user/subscription/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/user/order.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>orderConfirmation</from-outcome>
            <to-view-id>/user/orders.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/public/menu.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addCoursePhoto</from-outcome>
            <to-view-id>/manager/course/addPhoto.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>